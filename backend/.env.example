# PIM Backend Environment Variables
# Copy this to .env and fill in your values

# Groq API Keys (get from https://console.groq.com/keys - FREE!)
# With 3 keys: 90 RPM, 43,200 requests/day capacity
GROQ_API_KEY=gsk_your_primary_groq_key
GROQ_API_KEY_2=gsk_your_second_groq_key
GROQ_API_KEY_3=gsk_your_third_groq_key

# PostgreSQL Database URL (Railway provides this)
DATABASE_URL=postgresql://user:password@host:port/database

# Server Port
PORT=3000
