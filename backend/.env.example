# PIM Backend Environment Variables
# Copy this to .env and fill in your values

# Gemini API Keys (get from https://aistudio.google.com/app/apikey)
# Add multiple keys for automatic rotation when rate limited
GEMINI_API_KEY=your_primary_gemini_key
GEMINI_API_KEY_2=your_second_gemini_key
GEMINI_API_KEY_3=your_third_gemini_key
GEMINI_API_KEY_4=your_fourth_gemini_key
GEMINI_API_KEY_5=your_fifth_gemini_key

# PostgreSQL Database URL (Railway provides this)
DATABASE_URL=postgresql://user:password@host:port/database

# Server Port
PORT=3000
